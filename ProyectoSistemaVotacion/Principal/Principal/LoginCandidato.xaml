        <PasswordBox x:Name="passwordBox" HorizontalAlignment="Left" Margin="240,242,0,0" VerticalAlignment="Top" Width="120" Height="26"/>
        <Button x:Name="btnOut" Content="Salir" Margin="97,0,0,31.6" HorizontalAlignment="Left" Width="91" Height="32" VerticalAlignment="Bottom"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="114" Margin="160,50,0,0" VerticalAlignment="Top" Width="114" Source="Resources/administrator.png"/>

    </Grid>
</Window>
